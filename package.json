    "express-rate-limit": "^5.2.6",
    "fbgraph": "^1.4.4",
    "helmet": "^4.6.0",
    "imagemin": "7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "is-image": "^3.0.0",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "nestjs-i18n": "^8.1.1",
    "nestjs-typeorm-paginate": "^2.5.3",
    "pdfkit": "^0.12.1",
    "randomstring": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sharp": "^0.28.3",
    "slugify": "^1.5.3",
    "swagger-ui-express": "^4.1.6",
